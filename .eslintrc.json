{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": [".gitignore","serviceWorker.js"],
    "rules": {
        "react/jsx-filename-extension": [0] ,
        "jsx-a11y/no-noninteractive-element-interactions": [0],
        "jsx-a11y/anchor-is-valid": [0]
    }
}
